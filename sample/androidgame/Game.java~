class Main extends Activity {
    void onCreate() {
	this.addButton(1);
	this.addButton(2);
	this.addButton(3);
    }
    void onClick(int button) {
	if (button == 1) {
//L4:
	    Intent i = new Intent();
//L5:
	    i.setTarget("Game");
//L6:
	    this.startActivity(i);
	} else if (button == 2) {
//L7:
	    Intent i = new Intent();
//L8:
	    i.setTarget("Score");
//L9:
	    this.startActivity(i);
	} else if (button == 3) {
//L10:
	    Intent i = new Intent();
//L11:
	    i.setTarget("Help");
//L12:
	    i.setArg("Main");
//L13:
	    this.startActivity(i);
	} else {
	}
    }
}

